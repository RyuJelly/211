// step1
$(function(){
	$('#letter-a a').click(function(e){
		$('#dictionary').hide().load('a.html', function(){
			$(this).fadeIn();
		});
		
		e.preventDefault;
		// return false;
	});
});

//step2
$(function(){
	$('#letter-b a').click(function(e){
		$.getJSON('b.json', function(data){ 			// 변수안에 json값이 들어온다
			
		});
		
		e.preventDefault;
		// return false;
	});
});